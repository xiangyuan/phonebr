package com.wondershare.http.server;

public class ServletException extends Exception {

	/**
	 */
	private static final long serialVersionUID = -1539223095502572766L;

	public ServletException(String msg) {
		super(msg);
	}
	
	public ServletException(String msg,Throwable throwable) {
		super(msg, throwable);
	}
}
